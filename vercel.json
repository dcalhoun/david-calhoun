{
  "version": 2,
  "name": "davidcalhoun.me",
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "redirects": [
    {
      "source": "(/.*)+/",
      "destination": "$1",
      "statusCode": 307
    },
    {
      "source": "(/\\d{4}/\\d{2}/\\d{2}/)([^/]+)(/?)",
      "destination": "/blog/$2",
      "statusCode": 301
    },
    {
      "source": "/writing/(write|writing)-code-together",
      "destination": "/blog/building-software-together",
      "statusCode": 301
    },
    {
      "source": "/writing/migration-to-jekyll-my-journey-to-understanding-yeoman",
      "destination": "/blog/migration-to-jekyll-understanding-yeoman",
      "statusCode": 301
    },
    {
      "source": "/writing($|/.*)",
      "destination": "/blog$1",
      "statusCode": 301
    }
  ]
}
