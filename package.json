{
  "name": "dcalhoun.github.io",
  "version": "1.0.0",
  "description": "Personal site of David Calhoun.",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "build:export": "npm run build && npm run export",
    "build:posts": "processmd posts/**/*.md --outputDir .posts --preview 110 --highlightCode --summaryOutput .posts/summary.json",
    "deploy": "npm run build:posts && npm run build:export && mv out public",
    "export": "next export && cp -R static/* out/",
    "precommit": "lint-staged",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcalhoun/dcalhoun.github.io.git"
  },
  "author": "David Calhoun",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/dcalhoun/dcalhoun.github.io/issues"
  },
  "homepage": "http://davidcalhoun.me",
  "devDependencies": {
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.2.2",
    "processmd": "^3.1.3",
    "prop-types": "^15.5.10",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "chroma-js": "^1.3.4",
    "front-matter": "^2.1.2",
    "highlightjs": "^9.10.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^2.4.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-markdown": "^2.5.0",
    "styletron-client": "^2.5.7",
    "styletron-react": "^2.5.8",
    "styletron-server": "^2.5.7"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "standard --fix",
      "git add"
    ]
  }
}
